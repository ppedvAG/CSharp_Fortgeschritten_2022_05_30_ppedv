<Window x:Class="PluginClient.ParameterInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ParameterInput" Height="450" Width="200"
		SizeToContent="Height"
		FontSize="16">
    <StackPanel Margin="10">
		<TextBlock Text="Parameterliste" HorizontalAlignment="Center"/>
		<StackPanel Name="ParameterPanel"/>
		<Grid Height="30">
			<Grid.ColumnDefinitions>
				<ColumnDefinition/>
				<ColumnDefinition/>
			</Grid.ColumnDefinitions>
			<Button Click="AbbrechenClicked" Margin="0,0,5,0" Content="Abbrechen"/>
			<Button Click="OkClicked" Grid.Column="1" Margin="5,0,0,0" Content="Ok"/>
		</Grid>
	</StackPanel>
</Window>

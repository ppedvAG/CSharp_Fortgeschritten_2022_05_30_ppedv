<UserControl x:Class="PluginClient.ParameterElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<TextBlock Grid.Column="0" Name="ParameterName" Margin="0,0,5,0"/>
		<TextBox Grid.Column="1" Name="ParameterValue" Margin="5,0,0,0"/>
	</Grid>
</UserControl>
